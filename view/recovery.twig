<section class="container">
    <div id="message"></div>
    <div class="row" ng-if="password">
        <div class="col-sm-6 col-sm-offset-3 thanks-box">
            <h1>Success!</h1>
            <em>Your new password is [[ password ]]</em>
            <hr>
            <strong>What do you want to do next?</strong>
            <div class="btn-holder">
                <a ng-href="/#/" class="btn btn-default">Go to login page</a>
            </div>
        </div>
    </div>
</section>