<section class="container">
    <header class="page-title">
        <h1>Property Approval Request Form</h1>
    </header>
    <form action="#" class="approval-form">
        <div class="row">
            <div class="col-sm-4" >
                <input ng-model="address" class="form-control input-lg" id="searchPlace" type="text" placeholder="Property Address">
            </div>
            <div class="col-sm-2"><button class="btn btn-default btn-lg">Submit</button></div>
            <div class="col-sm-3 col-sm-offset-3 text-right">
                <a href="#" class="back" ng-click="goto($event, '/')">Back to dashboard</a>
            </div>
        </div>
        <div id="location"></div>
    </form>
</section>