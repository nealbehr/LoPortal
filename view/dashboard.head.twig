<header class="page-title">
    <h1>Dashboard</h1>
    <ul class="tabset">
        <li ng-repeat="tab in tabs" ng-class="{active: tab.isActive()}"><a href="/#[[ tab.path ]]">[[ tab.title ]]</a></li>
    </ul>
    <div class="btn-holder">
        <a href="#" class="btn btn-default" ng-click="createNewApproval($event)">New Property Approval</a>
        <a href="#" class="btn btn-default" ng-click="createListingFlyerRequest($event)">New Listing Flyer</a>
    </div>
</header>