container_commands:
  03createSymLink:
    command: /bin/bash .ebextensions/symlink.sh $PARAM1
  04npmInstall:
    command: npm install && node_modules/grunt-cli/bin/grunt
  05applyMigrations:
    command: php bin/phinx migrate -e $PARAM1
  06setupApache:
    command: "cp .ebextensions/enable_mod_deflate.conf /etc/httpd/conf.d/enable_mod_deflate.conf"
  07setupWkHtmlTox:
    command: yum -y install http://downloads.sourceforge.net/wkhtmltopdf/wkhtmltox-0.12.2.1_linux-centos6-amd64.rpm